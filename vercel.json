{
  "functions": {
    "api/**/*.cjs": {
      "runtime": "nodejs@18.x" 
      // Mude para 18.x, que é suportada há mais tempo
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/Front-Page.html"
    }
  ],
  "redirects": [
    {
      "source": "/Front-Page",
      "destination": "/Front-Page.html",
      "permanent": true
    },
    {
      "source": "/cadastro",
      "destination": "/cadastro.html",
      "permanent": true
    },
    {
      "source": "/homepage",
      "destination": "/homepage.html",
      "permanent": true
    }
  ]
}
