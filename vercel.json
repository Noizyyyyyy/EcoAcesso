{
  "functions": {
    "api/**/*.cjs": {
      "runtime": "nodejs@20.x" 
      // Mudança: Agora está "nodejs@20.x" em vez de "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/Front-Page.html"
    }
  ],
  "redirects": [
    {
      "source": "/Front-Page",
      "destination": "/Front-Page.html",
      "permanent": true
    },
    {
      "source": "/cadastro",
      "destination": "/cadastro.html",
      "permanent": true
    },
    {
      "source": "/homepage",
      "destination": "/homepage.html",
      "permanent": true
    }
  ]
}
