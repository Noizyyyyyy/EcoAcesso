{
  "functions": {
    "api/**/*.cjs": {
      "runtime": "nodejs" 
      // Sintaxe simplificada: deixa o Vercel escolher a vers√£o mais recente
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/Front-Page.html"
    }
  ],
  "redirects": [
    {
      "source": "/Front-Page",
      "destination": "/Front-Page.html",
      "permanent": true
    },
    {
      "source": "/cadastro",
      "destination": "/cadastro.html",
      "permanent": true
    },
    {
      "source": "/homepage",
      "destination": "/homepage.html",
      "permanent": true
    }
  ]
}
