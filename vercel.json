{
  "functions": {
    "api/**/*.cjs": {
      "runtime": "nodejs@20.x" 
      // Garante a sintaxe correta (com o '@') E a vers√£o suportada (20.x)
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/Front-Page.html"
    }
  ],
  "redirects": [
    {
      "source": "/Front-Page",
      "destination": "/Front-Page.html",
      "permanent": true
    },
    {
      "source": "/cadastro",
      "destination": "/cadastro.html",
      "permanent": true
    },
    {
      "source": "/homepage",
      "destination": "/homepage.html",
      "permanent": true
    }
  ]
}
